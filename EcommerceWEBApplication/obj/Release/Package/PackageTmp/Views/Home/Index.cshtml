                    <li class="category-item">
                        <a href="~/Products/ProductList?CategoryId=@v.Id&CategoryName=@v.Name">
                            <div class="category-icon"><img src="~/Resources/images/icon/@v.CategoryImage" alt="images" width="60px;" class="img-fluid"></div>
                            <span class="category-title">@v.Name</span>
                            <span class="category-quantity">(@v.TotalPosted)</span>
                        </a>
